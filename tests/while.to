MAKE INT i = 0;
DO {
    SET i++;
} WHILE (i < 10);
DO SAYLN (i); // 10

MAKE INT j = 0;
DO {
    DO {
        MAKE INT k = 5;
        BREAK WHEN (j >= k);
    };
    SET j++;
} WHILE (j < 10);

DO SAYLN (j); // 5

MAKE INT l = 0;
DO {
    SET l++;
    DO {
        MAKE INT m = 5;
        CONTINUE WHEN (l % 2 != 0);
        DO SAYLN (l);
    };
} WHILE (l < 10);
DO SAY ("Hello, world!"); // does a function
SET str = "Hello, world!"; // sets a variable
MAKE INT i = 0; // declares a variable
DEFINE add (INT a, INT b) {
    RETURN a + b;
}; // defines a function
IMPORT "demo.lib" AS demo; // imports an external library
INCLUDE "demo.to"; // includes another file, directly inlining it
RETURN 0; // returns a value
BREAK; // breaks out of a loop
CONTINUE; // continues to the next iteration of a loop

DO IF (i == 0) {
    DO SAY ("i is zero!");
} ELSE {
    DO SAY ("i is not zero!");
}; // does an if statement

DO {
    DO SAY (i);
    SET i += 1;
} WHILE (i < 10); // does a while loop

DO {
    DO SAY (i);
} FOR (RANGE(100) => i); // does a for loop

MAKE ARRAY INT arr = [1, 2, 3, 4, 5]; // makes an array
SET arr[0] = 10; // sets an element in an array

MAKE ANY a = 0; // makes a variable of any type
SET a = "Hello, world!"; // sets a variable of any type, switching its type
SET a = 123; // sets a variable of any type, switching its type

MAKE OBJECT obj = {
    "key": "value",
    "key2": 123,
    "key3": {
        "key4": "value2"
    },
    "key5": [1, 2, 3]
}; // makes an object

SET obj."key" = "new value"; // sets a key in an object
DO SAY (obj.key5[1]); // prints: 2
DO SAY (TYPE(obj.key5)); // prints: ARRAY
DO SAY (TYPE(obj.key3.key4)); // prints: STRING
DO SAY (TYPE(arr)); // prints: ARRAY INT (type is static, so it already knows the type of the array)